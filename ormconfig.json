{
   "type": "postgres",
   "host": "localhost",
   "port": 5432,
   "username": "postgres",
   "password": "docker",
   "database": "postgres",
   "synchronize": true,
   "dropSchema": true,
   "logging": false,
   "entities": [
      "packages/database-postgres/entity/**/*.js"
   ],
   "migrations": [
      "packages/database-postgres/migration/**/*.js"
   ],
   "subscribers": [
      "packages/database-postgres/subscriber/**/*.js"
   ],
   "cli": {
      "entitiesDir": "packages/database-postgres/entity",
      "migrationsDir": "packages/database-postgres/migration",
      "subscribersDir": "packages/database-postgres/subscriber"
   },
   "cache": {
      "type": "redis",
      "duration": 30000,
      "options": {
        "host": "localhost",
        "port": 6379
      }
   }
}